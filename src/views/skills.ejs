<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Portfolio</title>
  <%- include('partials/head.ejs') %>
</head>

<body>
  <!-- navbar -->
  <%- include('partials/navbar.ejs') %>

    <h1 class="skills-title">My Skills</h1>
    <div class="modal-skills">
      <div class="container">
        <div class="row">
          <div class="col-xl-5">
            <h3>html</h3>
            <div class="progress mt-2">
              <div class="progress-bar" role="progressbar" style="width: 80%" aria-valuenow="80%" aria-valuemin="0"
                aria-valuemax="100">
              </div>
            </div>
            <h3>css</h3>
            <div class="progress mt-2">
              <div class="progress-bar" role="progressbar" style="width: 70%" aria-valuenow="70" aria-valuemin="0"
                aria-valuemax="100"></div>
            </div>
            <h3>javascript</h3>
            <div class="progress mt-2">
              <div class="progress-bar" role="progressbar" style="width: 75%" aria-valuenow="75" aria-valuemin="0"
                aria-valuemax="100"></div>
            </div>
            <h3>Bootstrap</h3>
            <div class="progress mt-2">
              <div class="progress-bar" role="progressbar" style="width: 60%" aria-valuenow="60" aria-valuemin="0"
                aria-valuemax="100"></div>
            </div>
            <h3>node.js</h3>
            <div class="progress mt-2">
              <div class="progress-bar" role="progressbar" style="width: 25%" aria-valuenow="15" aria-valuemin="0"
                aria-valuemax="100"></div>
            </div>
            <h3>sql</h3>
            <div class="progress mt-2">
              <div class="progress-bar" role="progressbar" style="width: 70%" aria-valuenow="70" aria-valuemin="0"
                aria-valuemax="100"></div>
            </div>
          </div>
          <div class="col-xl-7">
            <div class="row">
              <div class="col-lg-4 image-skills glass">
                <div><img src="img/skills-img/html.png" alt="html img"></div>
              </div>
              <div class="col-lg-4 image-skills glass">
                <div><img src="img/skills-img/css.png" alt="css img"></div>
              </div>
              <div class="col-lg-4 image-skills glass">
                <div><img src="img/skills-img/js.png" alt="javascript img"></div>
              </div>
              <div class="col-lg-4 image-skills glass">
                <div><img src="img/skills-img/bootstrap.png" alt="bootstrap img"></div>
              </div>
              <div class="col-lg-4 image-skills glass">
                <div><img src="img/skills-img/node.png" alt="node.js img"></div>
              </div>
              <div class="col-lg-4 image-skills glass">
                <div><img src="img/skills-img/postgres.png" alt="postgreSQL img"></div>
              </div>
            </div>

          </div>
        </div>
      </div>
    </div>
    <!-- footer -->
    <%- include('partials/footer.ejs')%>
</body>

</html>